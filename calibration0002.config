aprilCameraCalibration {

    // MRE:   0.486 px MSE   0.307 px MaxRE   1.707 px MaxERE   3.265 px

    names = [ camera0000 ];

    camera0000 {
        class = "april.camera.models.AngularPolynomialCalibration";

        width = 2592;
        height = 1944;

        intrinsics {
            // focal length fx, fy
            fc = [2307.154295,2299.420209 ];
            // focal center cx, cy
            cc = [1362.314339,1115.581111 ];
            // polynomial approximation mapping theta to radius
            kc = [  -0.070026,  -0.127184,   0.231912,  -0.168761 ];
        }
        extrinsics {
            // Global-To-Camera coordinate transformation
            position = [   0.000000,   0.000000,   0.000000 ];
            rollpitchyaw_degrees = [   0.000000,  -0.000000,   0.000000 ];
        }
    }
}
