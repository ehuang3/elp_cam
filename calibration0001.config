aprilCameraCalibration {

    // MRE:   0.386 px MSE   0.208 px MaxRE   1.372 px MaxERE   3.256 px

    names = [ camera0000 ];

    camera0000 {
        class = "april.camera.models.AngularPolynomialCalibration";

        width = 2592;
        height = 1944;

        intrinsics {
            // focal length fx, fy
            fc = [2306.674079,2297.932654 ];
            // focal center cx, cy
            cc = [1359.036907,1070.781509 ];
            // polynomial approximation mapping theta to radius
            kc = [  -0.085153,  -0.036937,   0.009096,   0.020464 ];
        }
        extrinsics {
            // Global-To-Camera coordinate transformation
            position = [   0.000000,   0.000000,   0.000000 ];
            rollpitchyaw_degrees = [   0.000000,  -0.000000,   0.000000 ];
        }
    }
}
