aprilCameraCalibration {

    // MRE:   0.542 px MSE   0.385 px MaxRE   1.870 px MaxERE   3.164 px

    names = [ camera0000 ];

    camera0000 {
        class = "april.camera.models.AngularPolynomialCalibration";

        width = 2592;
        height = 1944;

        intrinsics {
            // focal length fx, fy
            fc = [2307.261861,2297.852776 ];
            // focal center cx, cy
            cc = [1384.423663,1164.155064 ];
            // polynomial approximation mapping theta to radius
            kc = [  -0.086517,  -0.060809,   0.102479,  -0.075601 ];
        }
        extrinsics {
            // Global-To-Camera coordinate transformation
            position = [   0.000000,   0.000000,   0.000000 ];
            rollpitchyaw_degrees = [   0.000000,  -0.000000,   0.000000 ];
        }
    }
}
